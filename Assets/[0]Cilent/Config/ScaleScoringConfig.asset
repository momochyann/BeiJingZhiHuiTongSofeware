        "\u7ED3\u679C\u6587\u6848": "\u5BB6\u5EAD\u6210\u5458\u7684\u89D2\u8272\u5206\u5DE5\u8F83\u5DEE\uFF0C\u4E0D\u80FD\u591F\u8BA4\u8BC6\u5230\u5404\u81EA\u7684\u5BB6\u5EAD\u804C\u8D23\uFF0C\u5BB9\u6613\u53D1\u751F\u89D2\u8272\u51B2\u7A81\u3002\u5B50\u5973\u7531\u4E8E\u7F3A\u4E4F\u953B\u70BC\uFF0C
          \u5F80\u5F80\u5BF9\u81EA\u5DF1\u6240\u5E94\u5F53\u626E\u6F14\u7684\u89D2\u8272\u6A21\u7CCA\u4E0D\u6E05\uFF0C
          \u4E0D\u61C2\u5F97\u81EA\u5DF1\u5F53\u524D\u7684\u4E3B\u8981\u4EFB\u52A1\u548C\u5E94\u627F\u62C5\u7684\u8D23\u4EFB\uFF0C\u5728\u53D7\u5230\u5916\u6765\u5F71\u54CD\u65F6\uFF0C\u7531\u4E8E\u7F3A\u4E4F\u8FA8\u522B\u529B\uFF0C
          \u5F80\u5F80\u6A21\u7CCA\u3002\u800C\u7236\u6BCD\u53EF\u80FD\u4E0D\u80FD\u4E3A\u5BB6\u4EBA\u63D0\u4F9B\u5FC5\u987B\u7684\u7269\u8D28\u6750\u6599\uFF0C\u8FD8\u53EF\u80FD\u76F8\u4E92\u63A8\u8BFF\u8D23\u4EFB\u3002\u5BB6\u4EBA\u5F97\u4E0D\u5230\u5F7C\u6B64\u7684\u60C5\u611F\u652F\u6301\u3002 "
        "\u5EFA\u8BAE": "\u5BB6\u5EAD\u6210\u5458\u89D2\u8272\u4E0D\u6E05\u6216\u6DF7\u4E71\uFF0C\u5EFA\u8BAE\u5BB6\u957F\u5E94\u8BE5\u8BA4\u8BC6\u5230\u5BB6\u957F\u662F\u5BB6\u5EAD\u6D3B\u52A8\u7684\u7EC4\u7EC7\u8005\u548C\u6307\u6325\u8005,
          \u662F\u5BB6\u5EAD\u529F\u80FD\u7684\u4E3B\u8981\u627F\u62C5\u8005, \u5BB6\u957F\u6240\u5C3D\u804C\u8D23\u7684\u597D\u574F,
          \u76F4\u63A5\u5F71\u54CD\u7740\u5BB6\u5EAD\u5404\u529F\u80FD\u7684\u53D1\u6325\u3002\u7236\u6BCD\u5E94\u8BE5\u52AA\u529B\u4E3A\u5BB6\u5EAD\u6210\u5458\u7684\u53D1\u5C55\u63D0\u4F9B\u5FC5\u8981\u7684\u7269\u8D28\u6761\u4EF6\uFF0C\u5B69\u5B50\u9664\u4E86\u8BA4\u771F\u5B66\u4E60\uFF0C\u8FD8\u5E94\u8BE5\u627F\u62C5\u4E00\u4E9B\u5BB6\u52A1\u52B3\u52A8\uFF0C\u51CF\u8F7B\u7236\u6BCD\u7684\u538B\u529B\u3002\u7236\u6BCD\u662F\u751F\u80B2\u517B\u80B2\u6211\u4EEC\u7684\u6069\u4EBA\uFF0C\u6CA1\u6709\u7236\u6BCD\uFF0C\u6211\u4EEC\u4E5F\u5C31\u65E0\u6CD5\u6765\u5230\u8FD9\u4E2A\u4E16\u754C\u3002\u56E0\u6B64\uFF0C\u5BF9\u5F85\u7236\u6BCD\uFF08\u5305\u62EC\u517B\u7236\u6BCD\uFF09\uFF0C\u8981\u5B66\u4F1A\u611F\u6069\u3002 "
